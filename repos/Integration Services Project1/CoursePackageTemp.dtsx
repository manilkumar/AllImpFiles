      <DTS:PropertyExpression
        DTS:Name="[ADO NET Source].[SqlCommand]">"SELECT * FROM (select distinct id,concat(SUBSTRING(co.created,1,7),'-',SUBSTRING(co.display_number_with_default,1,10)) as CourseCode,concat(SUBSTRING(co.created,1,7),'-',SUBSTRING(co.display_number_with_default,1,10)) as ModuleCode,
(CASE WHEN co.id like '%SG%' THEN 'SG' WHEN co.id like '%PH%' then 'PH' ELSE 'HK'  END)
 as CountryCode from course_overviews_courseoverview co
order by CountryCode) mytable where CountryCode='"+@[$Package::CountryCode] +"'"</DTS:PropertyExpression>
